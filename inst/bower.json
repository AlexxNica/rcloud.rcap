{
  "name": "RCAP",
  "version": "0.0.0",
  "description": "RCAP for RCloud",
  "keywords": [
    "rcloud",
    "r"
  ],
  "authors": [
    "Shane Porter",
    "Douglas Ashton"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "requirejs-plugins": "~1.0.3",
    "requirejs-text": "~2.0.14",
    "select2": "~4.0.0",
    "spectrum": "~1.8",
    "ionrangeslider": "~2.1.2",
    "datatables.net": "~1.10.10",
    "datatables.net-buttons": "~1.2.1",
    "datatables.net-buttons-dt": "~1.2.1",
    "jquery.sparkline": "~2.1.2",
    "datatables.net-fixedheader": "~3.1.2",
    "datatables.net-fixedheader-dt": "~3.1.2",
    "pubsub-js": "~1.5.2",
    "parsleyjs": "~2.7.2",
    "jquery.hotkeys": "*",
    "require-css": "~0.1.8"
  },
  "overrides": {
    "jquery.sparkline": {
      "main": "dist/jquery.sparkline.min.js" 
    },
    "parsleyjs": {
      "main": "dist/parsley.min.js"
    },
    "datatables.net": {
      "main": ["js/jquery.dataTables.min.js", 
        "js/jquery.dataTables.js"
      ]
    },
    "datatables.net-buttons": {
      "main": ["js/dataTables.buttons.js",
        "js/buttons.colVis.js",
        "js/buttons.flash.js",
        "js/buttons.html5.min.js",
        "js/buttons.print.js"
      ]
    },
    "select2": {
      "main": [
        "dist/js/select2.js", "dist/css/select2.min.css"
      ]
    },
    "ionrangeslider": {
      "main": [
        "js/ion.rangeSlider.js",
        "css/ion.rangeSlider.css",
        "css/ion.rangeSlider.skinFlat.css",
        "css/ion.rangeSlider.skinHTML5.css",        
        "img/sprite-skin-flat.png"
      ]
    }
  }
}
